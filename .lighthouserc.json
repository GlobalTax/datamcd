{
  "ci": {
    "collect": {
      "numberOfRuns": 1,
      "staticDistDir": "dist",
      "settings": {
        "budgetsPath": "public/lighthouse-budgets.json"
      }
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.9 }],
        "categories:accessibility": ["warn", { "minScore": 0.9 }],
        "categories:seo": ["warn", { "minScore": 0.9 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
